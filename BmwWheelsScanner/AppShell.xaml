<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="BmwWheelsScanner.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BmwWheelsScanner"
    Title="BmwWheelsScanner"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="BMW Wheel Scanner"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Wheel Details"
        ContentTemplate="{DataTemplate local:WheelDetailsPage}"
        Route="WheelDetailsPage" />

    <ShellContent
        Title="Google Search Wheel"
        ContentTemplate="{DataTemplate local:GoogleSearchWheel}"
        Route="GoogleSearchWheel" />

</Shell>
